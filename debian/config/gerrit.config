[gerrit]
	basePath = git
[database]
	type = MYSQL
	hostname = localhost
	database = reviewdb
	username = root
[auth]
	type = OPENID
[sendemail]
	smtpServer = localhost
[container]
	user = gerrit
	javaHome = /usr/lib/jvm/java-6-openjdk/jre
[sshd]
	listenAddress = *:29418
[httpd]
	listenUrl = http://*:8080/
[cache]
	directory = /var/cache/gerrit
